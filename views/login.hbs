<title>Login</title>
<div class="container-login100">
    <div class="wrap-login100">
        <form class="login100-form validate-form" action="/login" method="POST" id="loginForm">
            <span class="login100-form-title p-b-26">
                Welcome
            </span>
            <span class="login100-form-title p-b-26">
                <img src="./assets/img_avatar.png" width="150" height="150" alt="" />
            </span>

            {{#if err_message}}
            <div class="alert alert-warning alert-dismissible fade show" role="alert">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                {{err_message}}
            </div>
            {{/if}}
            <div class="wrap-input100 validate-input" data-validate="Username is required at least 6 characters.">
                <input class="input100" type="text" name="username" id="loginFormUsername" />
                <span class="focus-input100" data-placeholder="Username"></span>
            </div>

            <div class="wrap-input100 validate-input"
                data-validate="Password must contain at least 8 characters, 1 number, 1 upper and 1 lowercase.">
                <span class="btn-show-pass">
                    <i class="zmdi zmdi-eye"></i>
                </span>
                <input class="input100" type="password" name="password" id="loginFormPassword" />
                <span class="focus-input100" data-placeholder="Password"></span>
            </div>
            <div class="container-login100-form-btn">
                <div class="wrap-login100-form-btn">
                    <div class="login100-form-bgbtn"></div>
                    <button class="login100-form-btn" id="loginFormSubmit">
                        Login
                    </button>
                </div>
            </div>
            <div class="text-center p-t-115">
                <span class="txt1">
                    Donâ€™t have an account?
                </span>
                <a class="txt2" href="signup">
                    Sign Up
                </a>
                <a href="#">
                    <img src="./assets/images/ic_facebook.png" alt="" width="30">
                </a>
                <a href="#">
                    <img src="./assets/images/ic_google.png" alt="" width="30">
                </a>
            </div>
        </form>
    </div>
</div>

