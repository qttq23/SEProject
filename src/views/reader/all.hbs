<link rel="stylesheet" type="text/css" href="stylesheets/partials/items.css">
<script>
  $("#nav-all").className+="active";
</script>
<title>All Books</title>
<div class="item-section-container">
  <h4>All books</h4>
  <div class="item-section-container-flex">
    {{#if this.empty}}
  <div class="card-body">
    <h4 class="card-text">No data</h4>
  </div>
  {{else}}
      {{#each this.books}}
        {{>item this}}
      {{/each}}
  {{/if}}
  </div>
</div>
{{#if success}}
<script>
  alert("Book upload succeeded");
</script>
{{/if}}
